{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@bcrypt/bcrypt/(.*)": "<rootDir>/../libs/bcrypt/src/$1",
    "@bcrypt/bcrypt": "<rootDir>/../libs/bcrypt/src",
    "@encryption/encryption/(.*)": "<rootDir>/../libs/encryption/src/$1",
    "@encryption/encryption": "<rootDir>/../libs/encryption/src"
  }
}